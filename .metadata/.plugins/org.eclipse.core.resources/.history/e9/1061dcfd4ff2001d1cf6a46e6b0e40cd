#ifndef COLLISION_H_
#define COLLISION_H_


typedef enum CollisionType{
	NoCollision = 0,
	FrontalCollision,
	RearCollision,
	RightCollision,
	LeftCollision,

}CollisionType;
#include "car.h"


CollisionType DetectCollision();

bool hasCarParametersChanged();


#endif /* COLLISION_H_ */
