#ifndef COLLISION_H_
#define COLLISION_H_

#include "car.h"

typedef enum CollisionType{
	NoCollision = 0,
	FrontalCollision,
	RearCollision,
	RightCollision,
	LeftCollision,

}CollisionType;

CollisionType DetectCollision();

#endif /* COLLISION_H_ */
