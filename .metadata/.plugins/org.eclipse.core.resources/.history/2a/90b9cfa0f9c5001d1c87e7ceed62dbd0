#include "bluetooth.h"

volatile bool bluetooth_flag = false;
volatile VehicleControl bluetooth_data = 0;
void USART1_IRQHandler(void)
{
	bluetooth_flag = true;
	USART1->RXDAT;

    USART_ClearStatusFlags(USART1_PERIPHERAL, kUSART_RxReady);
}
