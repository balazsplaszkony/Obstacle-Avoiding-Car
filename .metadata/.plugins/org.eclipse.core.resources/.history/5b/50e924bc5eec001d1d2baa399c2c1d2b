#ifndef USART_H_
#define USART_H_

#define esc 0x1b
#define CR  0x0d
#define LF  0x0a

void INIT_USART(void);
void PrintUSART0_NB(char * str);
uint8_t GetUSART0(uint8_t *data);
uint8_t GetString_TillEndChar(char *str, uint8_t End_Char, uint8_t LenMax, uint8_t reset);

#endif /* USART_H_ */

