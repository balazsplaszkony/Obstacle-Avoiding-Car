typedef enum Direction{
	GOFORWARD,
	GOBACKWARD,
	TURNRIGHT,
	TURNLEFT,
	TURNRIGHTSTATIONARY,
	TURNLEFTSTATIONARY,
	STOPCAR,
}Direction;


void FindClearRoute();
